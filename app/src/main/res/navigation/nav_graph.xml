<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="net.kibotu.webviewactions.FirstFragment"
        android:label="@string/first_fragment"
        tools:layout="@layout/fragment_firstfragment">
        <action
            android:id="@+id/action_FirstFragment_to_WebViewFragment"
            app:destination="@id/WebViewFragment" />
    </fragment>

    <fragment
        android:id="@+id/WebViewFragment"
        android:name="net.kibotu.webviewactions.WebViewFragment"
        android:label="@string/webview_fragment"
        tools:layout="@layout/fragment_webview" />
</navigation>
